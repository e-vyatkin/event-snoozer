{
    "name": "zhem4ag/event-snoozer",
    "description": "Snooze your events for later dispatch.",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeny Vyatkin",
            "email": "vyatkinevgeny@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "symfony/event-dispatcher": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "suggest": {
        "symfony/console": "For launching console jobs",
        "doctrine/common": "Allows to save events in database"
    }
}
